  "scripts": {
    "check": "../../../scripts/node_modules/.bin/tsc --noEmit",
    "prep": "../../../scripts/prepare/bundle.ts"
  },
  "dependencies": {
    "@storybook/addons": "7.0.0-alpha.38",
    "@storybook/client-logger": "7.0.0-alpha.38",
