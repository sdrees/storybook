    "@types/ejs": "^3.1.1",
    "@yarnpkg/esbuild-plugin-pnp": "^3.0.0-rc.10",
    "browser-assert": "^1.2.1",
    "chalk": "^4.1.0",
    "ejs": "^3.1.8",
    "esbuild": "^0.14.48",
    "esbuild-plugin-alias": "^0.2.1",
