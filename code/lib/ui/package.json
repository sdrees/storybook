{
  "name": "@storybook/ui",
  "version": "7.0.0-alpha.31",
  "description": "Core Storybook UI",
  "keywords": [
    "storybook"
  ],
  "homepage": "https://github.com/storybookjs/storybook/tree/main/lib/ui",
  "bugs": {
    "url": "https://github.com/storybookjs/storybook/issues"
