  "scripts": {
    "check": "../../../scripts/node_modules/.bin/tsc --noEmit",
    "prep": "../../../scripts/prepare/bundle.ts"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@jest/transform": "^26.6.2",
